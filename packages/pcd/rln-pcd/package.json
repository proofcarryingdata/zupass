{
  "name": "@pcd/rln-pcd",
  "version": "0.8.0",
  "license": "GPL-3.0-or-later",
  "main": "./dist/node/index.js",
  "types": "./src/index.ts",
  "files": [
    "./artifacts/*",
    "./src/*",
    "dist",
    "./README.md",
    "./LICENSE"
  ],
  "exports": {
    ".": {
      "node": {
        "default": "./dist/node/index.js"
      },
      "browser": {
        "default": "./dist/browser/index.js"
      }
    }
  },
  "scripts": {
    "lint": "eslint \"**/*.ts{,x}\"",
    "test": "ts-mocha --config ../../../.mocharc.js --exit test/**/*.spec.ts",
    "build": "tsup --platform=browser src/index.ts --out-dir ./dist/browser && tsup --platform=node src/index.ts --out-dir ./dist/node",
    "dev": "echo concurrently \"yarn dev:browser\" \"yarn dev:node\"",
    "dev:browser": "tsup --platform=browser src/index.ts --out-dir ./dist/browser --format esm --dts --watch",
    "dev:node": "tsup --platform=node src/index.ts --out-dir ./dist/node --format cjs,esm --dts --watch",
    "typecheck": "yarn tsc --noEmit",
    "prepublishOnly": "yarn build",
    "clean": "rm -rf dist node_modules"
  },
  "dependencies": {
    "@pcd/pcd-types": "^0.9.0",
    "@pcd/semaphore-group-pcd": "^0.9.0",
    "@pcd/semaphore-identity-pcd": "^0.9.0",
    "@pcd/util": "0.3.0",
    "@semaphore-protocol/group": "^3.14.0",
    "@semaphore-protocol/identity": "^3.14.0",
    "json-bigint": "^1.0.0",
    "rlnjs": "^2.0.8",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@pcd/eslint-config-custom": "*",
    "@pcd/tsconfig": "*",
    "@types/json-bigint": "^1.0.1",
    "@types/mocha": "^10.0.1",
    "concurrently": "^8.2.0",
    "eslint": "^7.32.0",
    "mocha": "^10.2.0",
    "ts-mocha": "^10.0.0",
    "typescript": "^4.9.5"
  },
  "publishConfig": {
    "access": "public"
  }
}
