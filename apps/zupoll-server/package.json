{
  "name": "zupoll-server",
  "version": "0.0.12",
  "license": "GPL-3.0-or-later",
  "private": true,
  "scripts": {
    "build": "turbo run db:generate && tsc --noEmit",
    "db:generate": "prisma generate",
    "db:push": "prisma db push --skip-generate",
    "dev": "NODE_OPTIONS=--max_old_space_size=4096 NODE_ENV=development nodemon",
    "dev:https": "NODE_OPTIONS=--max_old_space_size=4096 NODE_ENV=development IS_LOCAL_HTTPS=true NODE_TLS_REJECT_UNAUTHORIZED=0 nodemon",
    "lint": "eslint \"**/*.ts{,x}\"",
    "pre-deploy": "./scripts/pre-deploy.sh",
    "start": "NODE_OPTIONS=--max_old_space_size=4096 NODE_ENV=production PORT=8080 ts-node src/main.ts",
    "test": "ts-mocha --type-check --exit test/**/*.spec.ts",
    "clean": "rm -rf out node_modules *.tsbuildinfo"
  },
  "dependencies": {
    "@honeycombio/opentelemetry-node": "^0.3.2",
